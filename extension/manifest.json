{
    "manifest_version": 3,
    "name": "Twitter Scraper Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/menu-burger(16).png",
            "32": "icons/menu-burger(32).png",
            "128": "icons/menu-burger(128).png"
        }
    }
}
